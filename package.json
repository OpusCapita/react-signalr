    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "global-jsdom": "4.2.0",
    "html-webpack-plugin": "2.30.1",
    "ignore-styles": "5.0.1",
    "immutable": "3.8.2",
    "jsdom": "13.1.0",
    "mocha": "5.2.0",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.5",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "2.1.6",
